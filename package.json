{
  "name": "nest-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start-core:dev": "set NODE_ENV=development && nest start --watch",
    "start-microservices:dev": "set NODE_ENV=development && nest start nest-microservices --watch",
    "build-core": "nest build",
    "build-microservices": "nest build nest-microservices",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prebuild": "rimraf dist",
    "prettier-fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "eslint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nest-core/test/jest-e2e.json",
    "create-module": "nest g mo module/posts",
    "create-controller": "nest g co module/posts",
    "create-child-controller": "nest g co module/example/example-test --no-spec --flat",
    "create-service": "nest g s module/posts",
    "create-filter": "nest g filter filter/http-exception",
    "create-interceptor": "nest g interceptor interceptor/transform",
    "create-middleware": "nest g middleware logger middleware",
    "create-guard": "nest g gu jwt guard",
    "create-pipe": "nest g pi name pipe",
    "create-crud": "nest g resource name"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
